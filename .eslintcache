ssages":"33","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"34","messages":"35","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/mohamedhamdan/Desktop/Projects/React/hiking-trips/src/components/TripList.js",["36","37","38"],"// Styling\nimport { ListWrapper, ThemeButton } from \"../styles\";\n\n// Components\nimport Trip from \"./Trip\";\nimport SearchBar from \"./SearchBar\";\nimport { useState } from \"react\";\nimport { Range, getTrackBackground } from \"react-range\";\nimport { useParams, Redirect, useHistory } from \"react-router-dom\";\nimport { Link } from \"react-router-dom\";\n\nconst TripList = (props) => {\n  const [query, setQuery] = useState(\"\");\n  const [currentMeasure, setMeasure] = useState(\"km\");\n  const [length, setLength] = useState({ values: [10] });\n  const [difficulty, setDifficulty] = useState(\"\");\n\n  const history = useHistory();\n\n  const STEP = 1;\n  const MIN = 1;\n  const MAX = 10;\n\n  const tripList = props.trips\n    .filter((trip) => trip.name.toLowerCase().includes(query.toLowerCase()))\n    .filter((trip) => trip.length <= parseInt(length.values[0].toFixed(1)))\n    .filter((trip) => trip.difficulty.includes(difficulty))\n    .map((trip) => (\n      <Trip currentMeasure={currentMeasure} trip={trip} key={trip.id} />\n    ));\n\n  const convert = () => {\n    if (currentMeasure === \"km\") {\n      setMeasure(\"mile\");\n    } else {\n      setMeasure(\"km\");\n    }\n  };\n\n  const handleChange = (e) => {\n    setDifficulty(e.target.value);\n    history.push(`/${e.target.value}`);\n  };\n\n  return (\n    <>\n      <ThemeButton onClick={convert}>Convert</ThemeButton>\n      <SearchBar setQuery={setQuery} />\n      <div\n        style={{\n          display: \"flex\",\n          justifyContent: \"center\",\n          flexWrap: \"wrap\",\n        }}\n      >\n        <Range\n          values={length.values}\n          step={STEP}\n          min={MIN}\n          max={MAX}\n          onChange={(values) => setLength({ values })}\n          renderMark={({ props, index }) => (\n            <div\n              {...props}\n              style={{\n                ...props.style,\n                height: \"16px\",\n                width: \"5px\",\n                backgroundColor:\n                  index * STEP < length.values[0] ? \"#548BF4\" : \"#ccc\",\n              }}\n            />\n          )}\n          renderTrack={({ props, children }) => (\n            <div\n              onMouseDown={props.onMouseDown}\n              onTouchStart={props.onTouchStart}\n              style={{\n                ...props.style,\n                height: \"36px\",\n                display: \"flex\",\n                width: \"100%\",\n              }}\n            >\n              <div\n                ref={props.ref}\n                style={{\n                  height: \"5px\",\n                  width: \"100%\",\n                  borderRadius: \"4px\",\n                  background: getTrackBackground({\n                    values: length.values,\n                    colors: [\"#548BF4\", \"#ccc\"],\n                    min: MIN,\n                    max: MAX,\n                  }),\n                  alignSelf: \"center\",\n                }}\n              >\n                {children}\n              </div>\n            </div>\n          )}\n          renderThumb={({ props, isDragged }) => (\n            <div\n              {...props}\n              style={{\n                ...props.style,\n                height: \"42px\",\n                width: \"42px\",\n                borderRadius: \"4px\",\n                backgroundColor: \"#FFF\",\n                display: \"flex\",\n                justifyContent: \"center\",\n                alignItems: \"center\",\n                boxShadow: \"0px 2px 6px #AAA\",\n              }}\n            >\n              <div\n                style={{\n                  height: \"16px\",\n                  width: \"5px\",\n                  backgroundColor: isDragged ? \"#548BF4\" : \"#CCC\",\n                }}\n              />\n            </div>\n          )}\n        />\n        <output style={{ marginTop: \"30px\" }}>\n          {parseInt(length.values[0].toFixed(1))}\n        </output>\n      </div>\n\n      <select\n        name=\"difficulties\"\n        id=\"difficulty-select\"\n        onChange={handleChange}\n      >\n        <option value=\"\">--Please choose a difficulty--</option>\n        <option value=\"Easy\">Easy</option>\n        <option value=\"Medium\">Medium</option>\n        <option value=\"Hard\">Hard</option>\n      </select>\n\n      <ListWrapper>{tripList}</ListWrapper>\n    </>\n  );\n};\n\nexport default TripList;\n",["39","40"],"/Users/mohamedhamdan/Desktop/Projects/React/hiking-trips/src/index.js",[],"/Users/mohamedhamdan/Desktop/Projects/React/hiking-trips/src/components/TripDetails.js",[],"/Users/mohamedhamdan/Desktop/Projects/React/hiking-trips/src/App.js",["41","42"],"/Users/mohamedhamdan/Desktop/Projects/React/hiking-trips/src/reportWebVitals.js",[],"/Users/mohamedhamdan/Desktop/Projects/React/hiking-trips/src/components/Trip.js",[],"/Users/mohamedhamdan/Desktop/Projects/React/hiking-trips/src/styles.js",[],"/Users/mohamedhamdan/Desktop/Projects/React/hiking-trips/src/components/RecomendedTrips.js",[],{"ruleId":"43","severity":1,"message":"44","line":9,"column":10,"nodeType":"45","messageId":"46","endLine":9,"endColumn":19},{"ruleId":"43","severity":1,"message":"47","line":9,"column":21,"nodeType":"45","messageId":"46","endLine":9,"endColumn":29},{"ruleId":"43","severity":1,"message":"48","line":10,"column":10,"nodeType":"45","messageId":"46","endLine":10,"endColumn":14},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","replacedBy":"52"},{"ruleId":"43","severity":1,"message":"53","line":4,"column":10,"nodeType":"45","messageId":"46","endLine":4,"endColumn":20},{"ruleId":"43","severity":1,"message":"54","line